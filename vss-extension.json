{
    "manifestVersion": 1,
    "id": "run-inline-csharp",
    "name": "Run Inline C#",
    "version": "0.0.0",
    "publisher": "JacobBundgaard",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Run C# code directly in your pipeline using .NET 10's file-based programs feature. Supports inline scripts and .cs files.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "runInlineCSharpTask"
        }
    ],
    "contributions": [
        {
            "id": "run-inline-csharp-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "runInlineCSharpTask"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/kimsey0/run-inline-csharp"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    }
}
